{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/auth/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/styles.css",
      "destination": "/server.js"
    },
    {
      "source": "/app.js",
      "destination": "/server.js"
    },
    {
      "source": "/",
      "destination": "/server.js"
    }
  ]
}